Program Example2; (* Factorial *)
   
Var n : Integer;
    b : Char;

Function F(n : Integer; x : Integer) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
   n:=b;
  For n := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI( F(n));
    End;
End. (* Factorial *)